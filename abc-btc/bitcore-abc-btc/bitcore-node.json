{
  "network": "livenet",
  "port": 3010,
  "services": [
    "bitcoind",
    "web",
    "insight-api",
    "insight-ui"
  ],
  "servicesConfig": {
    "insight-ui": {
      "routePrefix": "",
      "apiPrefix": "api"
    },
    "insight-api": {
      "routePrefix": "api"
    },
    "bitcoind": {
      "sendTxLog": "/opt/satoshilabs/bitcore-abc-btc/home/.bitcore/pushtx.log",
      "spawn": {
        "datadir": "/opt/satoshilabs/bitcore-abc-btc/home/.bitcore/data",
        "exec": "/opt/satoshilabs/bitcore-abc-btc/bitcore/node_modules/bitcore-node/bin/bitcoind",
        "rpcqueue": 1000,
        "rpcport": 8339,
        "zmqpubrawtx": "tcp://127.0.0.1:28339",
        "zmqpubhashblock": "tcp://127.0.0.1:28339"
      }
    }
  }
}

